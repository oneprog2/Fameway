{
  "expo": {
    "splash": {
      "image": "./src/assets/images/splash.png",
      "backgroundColor": "#222222",
      "resizeMode": "contain"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "https://auth.fameway.co/"
        }
      ]
    ],
    "scheme": "fameway",
    "userInterfaceStyle": "automatic",
    "android": {
      "userInterfaceStyle": "automatic"
    },
    "ios": {
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["fameway"]
      }
    }
  }
}